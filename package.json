{
  "name": "vself-beta-frontend",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev:server": "rm -rf data && rm -rf radata && yarn start:server",
    "start:server": "node service.js",
    "dev": "NODE_ENV=development next dev",
    "dev:hashdox": "env-cmd -f .env.development.hashdox next dev",
    "build": "next build",
    "build:prod": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "lint": "next lint",
    "test:watch": "jest --watch",
    "test": "jest -u",
    "deploy:dev": "env-cmd -f .env.development next build && firebase deploy -P staging --only hosting,functions:nextServer",
    "deploy:hashdox": "env-cmd -f .env.development.hashdox next build && firebase deploy -P hashdox --only hosting,functions:nextServer",
    "deploy:prod": "NODE_ENV=production next build && firebase deploy -P prod --only hosting,functions:nextServer"
  },
  "dependencies": {
    "@googlemaps/react-wrapper": "^1.1.30",
    "@reduxjs/toolkit": "^1.8.0",
    "@trpc/client": "^9.22.0",
    "@trpc/next": "^9.22.0",
    "@trpc/react": "^9.22.0",
    "@trpc/server": "^9.22.0",
    "axios": "^0.27.2",
    "exifr": "^7.1.3",
    "firebase": "^9.6.9",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.19.0",
    "gun": "^0.2020.1237",
    "js-base64": "^3.7.2",
    "js-sha3": "^0.8.0",
    "near-api-js": "^0.44.2",
    "next": "12.1.0",
    "next-i18next": "^11.0.0",
    "next-themes": "^0.1.1",
    "react": "17.0.2",
    "react-datepicker": "^4.7.0",
    "react-dom": "17.0.2",
    "react-image-file-resizer": "^0.4.7",
    "react-page-scroller": "^2.2.1",
    "react-query": "^3.34.19",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "shelljs": "^0.8.4",
    "socket.io": "^4.4.0",
    "tailwindcss-animation-delay": "^1.0.7",
    "tw-elements": "^1.0.0-alpha11",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@types/google.maps": "^3.48.5",
    "@types/jest": "^27.4.1",
    "@types/react": "17.0.42",
    "@types/react-datepicker": "^4.3.4",
    "@types/react-redux": "^7.1.23",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "autoprefixer": "^10.4.4",
    "env-cmd": "^10.1.0",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.1.4",
    "typescript": "4.6.2"
  }
}
